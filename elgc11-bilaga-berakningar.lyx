\begin_layout Standard
\begin_inset Formula 
\begin{equation}
A_{0}=\frac{MTBM}{MTBM+MLDT+MTW(A)+\overline{M}}\label{eq:operativ_tillganglighet}
\end{equation}

\end_inset


